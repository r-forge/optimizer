source("/home/john/current/1811Rcgmin/Rcgmin2/R/Rcgminu.R")
n <- length(x0)
ll <- rep(-1000, n)
uu <- -ll
mm <- rep(1,n)
rrtest<-Rcgminu(x0, rbf, rbg,  control=list(trace=4))
