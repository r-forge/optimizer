source('~/current/1811Rcgmin/Rcgmin2/R/Rcgminb.R', echo=TRUE)
require(funconstrain)
rb <- rosen()
rbf <- rb$fn
rbg <- rb$gr
x0 <- rb$x0
fmin <- rbf(x0)
cat("f(x0)=", fmin,"\n")
require(numDeriv)
gn <- grad(rbf, x0)
ga <- rbg(x0)
max(abs(gn-ga))
