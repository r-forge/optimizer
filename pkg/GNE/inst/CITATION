citHeader("To cite GNE in publications use:")


citEntry(entry="Manual",
         title = "GNE: computation of Nash Equilibria",
         author = "Christophe Dutang",
	 year = 2013,
         note = "R package version 0.97",
         textVersion =
	 paste(	"C. Dutang",
		"(2013),",
	       	"GNE: computation of generalized Nash Equilibria")
)

